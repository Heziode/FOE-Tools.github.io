{
  "name": "foe-tools",
  "version": "2.81.1",
  "description": "Set of tools for Forge of Empires",
  "main": "nuxt.config.js",
  "engines": {
    "node": ">= 12.13.0"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:prod": "cross-env NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
    "prettierfix": "prettier --write .",
    "build:gh": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "release": "standard-version",
    "deploy": "push-dir --dir=dist --branch=master --cleanup",
    "coverage": "codecov",
    "contributors:add": "all-contributors --config all-contributorsrc.json add",
    "contributors:generate": "all-contributors --config all-contributorsrc.json generate",
    "test:lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "test:prettier": "prettier --check .",
    "test:unit": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=false",
    "test:unit:coverage": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=true --coverageReporters=lcov",
    "test:unit:coverage:dev": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=true",
    "test:unit:watch": "NODE_OPTIONS=--max_old_space_size=4096 jest --clearCache && NODE_OPTIONS=--max_old_space_size=4096 jest --coverage=false --testRegex ^.*.test.js$ test/unit/ --watch",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 npm run test:unit",
    "update-foe-data": "node .scripts/update-gb-data.js",
    "i18n:watch": "vuei18n-po --watch --messagesDir=lang translations/*.po",
    "i18n:generate": "vuei18n-po --messagesDir=lang translations/*.po"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FOE-Tools/FOE-Tools.github.io.git"
  },
  "keywords": [
    "foe",
    "forge",
    "empire",
    "nuxt",
    "vue",
    "tool",
    "forge-of-empires",
    "investment"
  ],
  "author": "Heziode <Heziode@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FOE-Tools/FOE-Tools.github.io/issues"
  },
  "homepage": "https://foe-tools.github.io/",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime-corejs3": "^7.12.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nuxt/babel-preset-app": "^2.14.12",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
